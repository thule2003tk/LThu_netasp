
@{
    ViewBag.Title = "Danhh sách khách hàng";
    var list = ViewBag.list;
    //var list = Model as list<ThuLesson04.Models.Thucustomer>;
    int stt = 0;
}

<h2>Danh sách khách hàng</h2>
<div class="row">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>Mã số </th>
                <th>Họ và tên</th>
                <th>Địa chỉ </th>
                <th>Năm sinh</th>
                <th>Chức năng</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in list)
            {
                stt++;
                <tr>
                    <td>@stt</td>
                    <td>@item.CustomerId</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.Address</td>
                    <td>@item.YearOfBirth</td>
                    <td>
                        <a href="#"> Edit</a>
                        <a href="#">Delete</a>
                    </td>

                </tr>}
            
        </tbody>
    </table>
</div>

